{
    "name": "@redwoodjs/starter-standard",
    "version": "1.0.0",
    "description": "Build fast, server-driven webapps on Cloudflare with SSR, RSC, and realtime. ",
    "main": "index.js",
    "type": "module",
    "keywords": [],
    "author": "",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "vite build",
        "dev": "NODE_ENV=${NODE_ENV:-development} vite dev",
        "dev:init": "rw-scripts dev-init",
        "preview": "vite preview",
        "worker:run": "rw-scripts worker-run",
        "clean": "npm run clean:vite",
        "clean:vite": "rm -rf ./node_modules/.vite",
        "release": "npm run clean && npm run build && wrangler deploy",
        "migrate:new": "drizzle-kit generate",
        "migrate:dev": "wrangler d1 migrations apply DB --local",
        "migrate:prod": "wrangler d1 migrations apply DB --remote",
        "seed": "npm run worker:run ./src/db/seed.ts"
    },
    "dependencies": {
        "@simplewebauthn/browser": "^13.2.2",
        "@simplewebauthn/server": "^13.2.2",
        "@tailwindcss/vite": "^4.1.15",
        "bcryptjs": "^3.0.2",
        "dotenv": "^17.2.3",
        "drizzle-orm": "^0.44.6",
        "jose": "^6.1.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "rwsdk": "1.0.0-beta.9",
        "tailwindcss": "^4.1.15",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@cloudflare/vite-plugin": "1.13.10",
        "@cloudflare/workers-types": "^4.20251014.0",
        "@types/node": "^22.18.12",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react": "^5.0.4",
        "babel-plugin-react-compiler": "19.1.0-rc.3",
        "drizzle-kit": "^0.31.5",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "vibe-rules": "^0.2.57",
        "vite": "^7.1.11",
        "wrangler": "~4.42.2"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@prisma/client",
            "@prisma/engines",
            "esbuild",
            "prisma",
            "sharp",
            "workerd"
        ]
    }
}